{
    "manifest_version": 3,
    "name": "简洁地址栏",
    "version": "1.0",
    "description": "在新标签页中心显示一个简单的地址栏",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icons/*.svg"
            ],
            "matches": []
        }
    ]
}